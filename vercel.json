{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/books/recommendations",
      "dest": "/index.js"
    },
    {
      "src": "/books",
      "methods": ["GET", "POST"],
      "dest": "/index.js"
    },
    {
      "src": "/books/(?<id>[^/]+)",
      "methods": ["PUT", "DELETE"],
      "dest": "/index.js"
    }
  ]
}
